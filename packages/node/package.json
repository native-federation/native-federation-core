{
  "name": "@softarc/native-federation-node",
  "version": "4.0.0-RC2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/node/dist",
          "main": "packages/node/src/index.ts",
          "tsConfig": "packages/node/tsconfig.lib.json",
          "format": [
            "esm"
          ],
          "declarationRootDir": "packages/node/src"
        }
      }
    }
  },
  "dependencies": {
    "@softarc/native-federation": "4.0.0-RC2",
    "@softarc/native-federation-runtime": "4.0.0-RC2"
  }
}
