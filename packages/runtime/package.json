{
  "name": "@softarc/native-federation-runtime",
  "version": "4.0.0-RC1",
  "type": "module",
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    }
  },
  "files": [
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "devDependencies": {
    "@types/node": "^22.5.4",
    "vitest": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "@types/trusted-types": "^2.0.7"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --project=unit",
    "test:integration": "vitest run --project=integration"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
